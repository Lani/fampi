    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.0",
    "babel-register": "^6.24.0",
    "license-compatibility-checker": "^0.3.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "standard": "^9.0.1"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "cross-env": "^3.2.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
